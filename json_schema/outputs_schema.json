    "TokenIdValue": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "pop"
          ]
        },
        {
          "type": "object",
          "required": [
            "push"
          ],
          "properties": {
            "push": {
              "$ref": "#/definitions/U256"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "extend"
          ],
          "properties": {
            "extend": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/U256"
              }

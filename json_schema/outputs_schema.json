      "oneOf": [
        {
          "type": "object",
          "required": [
            "tokenUpdate"
          ],
          "properties": {
            "tokenUpdate": {
              "$ref": "#/definitions/TokenUpdate"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "programUpdate"
          ],
          "properties": {
            "programUpdate": {
              "$ref": "#/definitions/ProgramUpdate"
            }

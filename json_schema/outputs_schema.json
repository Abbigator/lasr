          }
        },
        "updateFields": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/TokenUpdateField"
          }
        }
      }
    },
    "TokenField": {
      "type": "string",
      "enum": [
        "programId",
        "ownerId",
        "balance",
        "metadata",
        "tokenIds",
        "allowance",
        "approvals",
        "data",
        "status"
      ]
    },
    "TokenFieldValue": {
      "oneOf": [
        {
          "type": "object",
          "required": [
            "balance"
          ],
          "properties": {
            "balance": {
              "$ref": "#/definitions/BalanceValue"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "metadata"
          ],
          "properties": {
            "metadata": {

            }
          },
          "additionalProperties": false
        }
      ]
    },
    "ProgramUpdate": {
      "type": "object",
      "required": [
        "account",
        "updates"
      ],
      "properties": {
        "account": {
          "$ref": "#/definitions/AddressOrNamespace"
        },
        "updates": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/ProgramUpdateField"
          }
        }
      }
    },
    "ProgramUpdateField": {
      "type": "object",
      "required": [
        "field",

            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "tokenIds"
          ],
          "properties": {
            "tokenIds": {
              "$ref": "#/definitions/TokenIdValue"
            }
          },
          "additionalProperties": false
        },
        {
          "type": "object",
          "required": [
            "allowance"
          ],
          "properties": {
            "allowance": {
              "$ref": "#/definitions/AllowanceValue"
            }
          },
          "additionalProperties": false
